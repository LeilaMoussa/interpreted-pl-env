~ Complexity level: 3

func (num grade) => get_letter => (ascii) [
	check (gt(grade, 90)) [
		give 'A'.
	]
	other [
		give 'B'.
	]
	~ might get rid of check other and do something simpler
]

() => entry => () [
	var num grade.
	var ascii letter.

	grade := 100.
	letter := (grade) => get_letter.
	(letter) => write.
]
